    "karma-moxios": "^0.1.0",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
